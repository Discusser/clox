var a = 3;

// Successive break/continue statements are not an error because a break/continue 
// is simply a jump instruction, so it would be pointless to disallow it.
switch (a) {
  case 0:
    print 0;
    break;
  case 3:
    print 3;
  case 1:
    print 1;
    break;
    break;
  case 2:
    print 2;
    break;
}
